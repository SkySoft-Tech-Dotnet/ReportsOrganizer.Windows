<UserControl x:Class="ReportsOrganizer.UI.Views.SettingsView"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
             xmlns:materialDesign="http://materialdesigninxaml.net/winfx/xaml/themes"
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
             xmlns:view="clr-namespace:ReportsOrganizer.UI.Views"
             mc:Ignorable="d"
             DataContext="{Binding SettingsPage, Source={StaticResource ViewModelLocator}}">
    <Grid Height="151" Width="436">
        <Grid.ColumnDefinitions>
            <ColumnDefinition/>
            <ColumnDefinition Width="67*"/>
        </Grid.ColumnDefinitions>
        <Button Command="{Binding BackCommand}" Style="{DynamicResource MaterialDesignFloatingActionMiniButton}" Width="50" Height="50" Grid.Column="1" Margin="20,10,360,91">
            <materialDesign:PackIcon x:Name="packIcon" Kind="Backspace" Height="Auto" Margin="10" Width="Auto" />
        </Button>
        <TextBox Grid.Column="1" HorizontalAlignment="Left" Height="23" Margin="164,30,0,0" TextWrapping="Wrap" Text="TextBox" VerticalAlignment="Top" Width="120"/>
        <Label Content="Timer" Grid.Column="1" HorizontalAlignment="Left" Margin="106,29,0,0" VerticalAlignment="Top"/>


    </Grid>
</UserControl>
