<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:materialDesign="http://materialdesigninxaml.net/winfx/xaml/themes"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">
    <Style TargetType="ComboBox" x:Key="DateComboBox" BasedOn="{StaticResource MaterialDesignFloatingHintComboBox}">
        <Setter Property="HorizontalAlignment" Value="Right" />
        <Setter Property="VerticalAlignment" Value="Center" />
        <Setter Property="Width" Value="185" />
        <Setter Property="Height" Value="35" />
        <Setter Property="FontSize" Value="15" />
        <Setter Property="Padding" Value="10,0" />
        <Setter Property="Margin" Value="10,0" />
    </Style>
    <Style TargetType="Button" x:Key="AddReportButton" BasedOn="{StaticResource MaterialDesignFloatingActionLightButton}">
        <Setter Property="Width" Value="50" />
        <Setter Property="Height" Value="50" />
        <Setter Property="ToolTip" Value="MaterialDesignFloatingActionLightButton" />
    </Style>
    <!--<Style TargetType="materialDesign:PackIcon" x:Key="AddReportIconButton">
        <Setter Property="Width" Value="25" />
        <Setter Property="Height" Value="25" />
        <Setter Property="Kind" Value="Plus" />
    </Style>-->
</ResourceDictionary>